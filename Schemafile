# frozen_string_literal: true
# encoding: utf-8

create_table "users", force: :cascade do |t|
  t.string   "email", null: false
  t.string   "password_digest", null: false
  t.timestamps
end

create_table "onsens", force: :cascade do |t|
  t.string   "jalan_onsen_id", null: false
  t.string   "name", null: false
  t.string   "location"
  t.text     "description"
  t.timestamps
end

create_table "favorites", force: :cascade do |t|
  t.bigint   "user_id"
  t.bigint   "onsen_id"
  t.timestamps
end

create_table "visit_logs", force: :cascade do |t|
  t.bigint   "user_id"
  t.bigint   "onsen_id"
  t.date     "visited_date", null: false
  t.text     "comment"
  t.timestamps
end

create_table "wishlists", force: :cascade do |t|
  t.bigint   "user_id"
  t.bigint   "onsen_id"
  t.timestamps
end
